{
	"name": "@michealpearce/utils",
	"version": "0.0.5",
	"license": "Unlicense",
	"author": {
		"name": "Micheal Pearce",
		"email": "michealpearce@outlook.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/MichealPearce/utils.git"
	},
	"bugs": {
		"url": "https://github.com/MichealPearce/utils/issues"
	},
	"main": "dist/esm/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"require": "./dist/cjs/index.js",
			"import": "./dist/esm/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist",
		"package.json",
		"README.md",
		"LICENSE"
	],
	"scripts": {
		"dev": "nodemon",
		"build": "bash ./scripts/build.sh",
		"test": "echo 'running test...' && ts-node test/index.ts"
	},
	"nodemonConfig": {
		"exec": "run-s build test",
		"ext": "ts",
		"watch": [
			"src",
			"test"
		]
	},
	"devDependencies": {
		"@types/node": "^18.13.0",
		"nodemon": "^2.0.20",
		"npm-run-all": "^4.1.5",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.1.2",
		"typescript": "^4.9.3"
	}
}
